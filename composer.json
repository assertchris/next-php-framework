{
  "name": "next/framework",
  "description": "Lightweight framework for PHP app development",
  "scripts": {
    "test": "vendor/bin/pest",
    "test:coverage": "vendor/bin/pest --coverage"
  },
  "require": {
    "filp/whoops": "^2.9",
    "illuminate/container": "^8.15",
    "illuminate/database": "^8.15",
    "illuminate/http": "^8.15",
    "illuminate/log": "^8.15",
    "nikic/fast-route": "^1.3",
    "nunomaduro/collision": "^5.1",
    "rakit/validation": "^1.4",
    "symfony/cache": "^5.1"
  },
  "require-dev": {
    "pestphp/pest": "^0.3.12",
    "phpunit/phpunit": "^9.3.10"
  },
  "autoload": {
    "files": [
      "./source/helpers.php"
    ],
    "psr-4": {
      "Next\\": "source"
    }
  },
  "config": {
    "sort-packages": true
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
